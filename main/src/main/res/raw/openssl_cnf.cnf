#
# OpenSSL configuration file for Android PQC VPN
#
openssl_conf = openssl_init

[openssl_init]
providers = provider_sect

[provider_sect]
default = default_sect
oqsprovider = oqsprovider_sect

[default_sect]
activate = 1

[oqsprovider_sect]
activate = 1
# ### DEFINITIVE FIX ###
# On Android, all .so files are in the same directory.
# We must point directly to the module name, not a subdirectory.
module = oqsprovider.so

# --- The rest of the file can be removed for the client ---
# --- as it is only needed for certificate generation.     ---